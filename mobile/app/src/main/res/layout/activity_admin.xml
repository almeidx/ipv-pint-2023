<?xml version="1.0" encoding="utf-8" ?>
<ScrollView
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/bg"
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
>

	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/layoutX"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:paddingBottom="30dp"
		tools:context=".pages.AdminActivity"
	>

    <TextView
			android:id="@+id/Beneficios"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="36dp"
			android:layout_marginStart="10dp"
			android:layout_marginEnd="10dp"
			android:paddingVertical="10dp"
			android:paddingStart="10dp"
			android:paddingEnd="10dp"
			android:background="@drawable/rounded"
			android:text="Beneficios"
			android:drawableEnd="@drawable/arrow_right"
			android:onClick="gotoAdminBeneficios"
			android:textSize="20sp"
			android:textStyle="bold"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
		/>

    <TextView
			android:id="@+id/Candidaturas"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="36dp"
			android:layout_marginStart="10dp"
			android:layout_marginEnd="10dp"
			android:paddingVertical="10dp"
			android:paddingStart="10dp"
			android:paddingEnd="10dp"
			android:background="@drawable/rounded"
			android:text="Candidaturas"
			android:drawableEnd="@drawable/arrow_right"
			android:onClick="gotoAdminCandidaturas"
			android:textSize="20sp"
			android:textStyle="bold"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/Beneficios"
		/>

    <TextView
			android:id="@+id/Ideias"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="36dp"
			android:layout_marginStart="10dp"
			android:layout_marginEnd="10dp"
			android:paddingVertical="10dp"
			android:paddingStart="10dp"
			android:paddingEnd="10dp"
			android:background="@drawable/rounded"
			android:text="Ideias"
			android:drawableEnd="@drawable/arrow_right"
			android:onClick="gotoAdminIdeias"
			android:textSize="20sp"
			android:textStyle="bold"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/Candidaturas"
		/>

    <TextView
			android:id="@+id/Mensagens"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="36dp"
			android:layout_marginStart="10dp"
			android:layout_marginEnd="10dp"
			android:paddingVertical="10dp"
			android:paddingStart="10dp"
			android:paddingEnd="10dp"
			android:background="@drawable/rounded"
			android:text="Mensagens"
			android:drawableEnd="@drawable/arrow_right"
			android:onClick="gotoAdminMensagens"
			android:textSize="20sp"
			android:textStyle="bold"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/Ideias"
		/>

    <TextView
			android:id="@+id/Negocios"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="36dp"
			android:layout_marginStart="10dp"
			android:layout_marginEnd="10dp"
			android:paddingVertical="10dp"
			android:paddingStart="10dp"
			android:paddingEnd="10dp"
			android:background="@drawable/rounded"
			android:text="Negócios"
			android:drawableEnd="@drawable/arrow_right"
			android:onClick="gotoAdminNegocios"
			android:textSize="20sp"
			android:textStyle="bold"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/Mensagens"
		/>


    <TextView
			android:id="@+id/Reporting"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="36dp"
			android:layout_marginStart="10dp"
			android:layout_marginEnd="10dp"
			android:paddingVertical="10dp"
			android:paddingStart="10dp"
			android:paddingEnd="10dp"
			android:background="@drawable/rounded"
			android:drawableEnd="@drawable/arrow_right"
			android:text="Reporting"
			android:textStyle="bold"
			android:textSize="20sp"
			android:onClick="gotoAdminReporting"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/Negocios"
		/>

    <TextView
			android:id="@+id/Reunioes"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="36dp"
			android:layout_marginStart="10dp"
			android:layout_marginEnd="10dp"
			android:paddingVertical="10dp"
			android:paddingStart="10dp"
			android:paddingEnd="10dp"
			android:background="@drawable/rounded"
			android:text="Reuniões"
			android:drawableEnd="@drawable/arrow_right"
			android:onClick="gotoAdminReunioes"
			android:textSize="20sp"
			android:textStyle="bold"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/Reporting"
		/>


    <TextView
			android:id="@+id/Utilizador"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="36dp"
			android:layout_marginStart="10dp"
			android:layout_marginEnd="10dp"
			android:paddingVertical="10dp"
			android:paddingStart="10dp"
			android:paddingEnd="10dp"
			android:background="@drawable/rounded"
			android:text="Utilizadores"
			android:drawableEnd="@drawable/arrow_right"
			android:onClick="gotoAdminUtilizadores"
			android:textSize="20sp"
			android:textStyle="bold"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/Reunioes"
		/>

    <TextView
			android:id="@+id/Vaga"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="36dp"
			android:layout_marginStart="10dp"
			android:layout_marginEnd="10dp"
			android:paddingVertical="10dp"
			android:paddingStart="10dp"
			android:paddingEnd="10dp"
			android:background="@drawable/rounded"
			android:text="Vagas"
			android:drawableEnd="@drawable/arrow_right"
			android:onClick="gotoAdminVagas"
			android:textSize="20sp"
			android:textStyle="bold"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/Utilizador"
		/>
	</androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
